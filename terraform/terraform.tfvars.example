# Example Terraform variables for Dropbox transcription pipeline
# Copy this to terraform.tfvars and fill in your values

# Required: Your GCP project ID
project_id = "your-gcp-project-id"

# Optional: GCP region (defaults to us-east1)
region = "us-east1"

# Required: Dropbox app access token (create at https://www.dropbox.com/developers/apps)
dropbox_access_token = "your-dropbox-access-token"

# Required: Dropbox app secret for webhook verification
dropbox_app_secret = "your-dropbox-app-secret"